<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Expedia" parallel="false">

    <parameter name="cloud" value="false"/>
    <parameter name="envName" value="browserstack"/>
    <parameter name="platform" value="mac"/>
    <parameter name="os_version" value="High Sierra"/>
    <parameter name="browser" value="chrome"/>
    <parameter name="browserVersion" value="74.0"/>
    <parameter name="url" value="https://www.expedia.com/"/>



    <test name="Expedia Home">
        <classes>
            <class name="regression.TestSearchPage"/>
            <class name="testPageClasses2.TestExpediaFlights"/>
            <class name="testPageClasses3.testExpediaHotels"/>
            <class name="testPageClasses4.testExpediaCars"/>
            <class name="testPageClasses5.TestExpediaBundleAndSave"/>
            <class name="testPageClasses6.testExpediaCruises"/>
            <class name="testPageClasses7.TestExpediaThingsToDo"/>
            <class name="testPageClasses8.TestExpediaVacationRentals"/>
            <class name="testPageClasses9.TestExpediaDeals"/>
            <class name="testPageClasses10.TestExpediaMobile"/>

        </classes>
    </test>
</suite>
